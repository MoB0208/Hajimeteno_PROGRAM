<!--管理者用model:Contentの検索結果一覧用-->
<% if content.npost.any? %>
  <% content.post.each do |content| %>
  <div class="col rounded bg-light my-3 p-2">
    <h4><%= link_to content.post.main_code, admins_post_path(content.post) %></h4>
    <p><%= content.post.title %></p>
    <p>
      <%= image_tag content.post.user.get_image, size: "50x50" %>
      <%= content.post.user.account_name %>
      <%= content.post.created_at.strftime('%Y/%m/%d') %>
    </p>
  </div>
  <% end %>
<% else %>
  <p>該当する記事はありません。</p>
<% end %>