<!--管理者用検索機能用-->
<%= form_with url: admins_search_path, method: :get, local: true do |f| %>
  <%= f.text_field :content %>
  <%= f.select :model, options_for_select({ "タイトル" => "Post", "もくじ" => "Content", "本文" => "MainText", "コード" => "PostCode" }) %>
  <!--<%#= f.select :method, options_for_select({ "完全一致" => "perfect", "前方一致" => "forward", "後方一致" => "backward", "部分一致" => "partial" }) %>-->
  <%= f.submit :search, class: "btn btn-light" %>
<% end %>