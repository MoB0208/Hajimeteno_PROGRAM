<!--model:MainTextの検索結果一覧-->
<% if main_text %>
  <% main_text.each do |main_text| %>
  <div class="col rounded bg-white my-5 p-5">
    <h4><%= link_to main_text.post.main_code, post_path(main_text.post) %></h4>
    <p><%= main_text.post.title %></p>
    <p>
      <%= image_tag main_text.post.user.get_image, size: "30x30" %>
      <%= main_text.post.user.account_name %>
      <%= main_text.post.created_at.strftime('%Y/%m/%d') %>
      <%#= render '/users/favorites/form', post: post %>
    </p>
  </div>
  <% end %>
<% else %>
  <p>該当する記事はありません</p>
<% end %>