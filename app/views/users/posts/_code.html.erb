<!--PostCodeから引用した記事-->
<table class='table table-hover table-inverse'>
  <thead>
    <tr>
      <th></th>
      <th>Title</th>
      <th>Main Code</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% post_codes.each do |post_code| %>
      <tr>
        <td><%= link_to post_code.post.main_code, posts_show_path %></td>
        <td><%= post_code.post.title %></td>
        <td><% render '/favorites/form' %></td>
      </tr>
    <% end %>
  </tbody>
</table>